{
  "manifest_version": 2,
  "name": "Sample Web Scrapping Insurance Checker",
  "version": "1.0.0",
  "description": "This is a simple extensiong for web scrapping",
  "author": "Mark Gocela",
  "icons": {
    "128": "square-logo.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "square-logo.png",
    "default_title": "web scraping"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": ["tabs"],
  "background": [
    {
      "script": ["jquery.min.js", "main.js"],
      "persistent": false
    }
  ]
}
  